<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stockproject.controller.ProductEditController">
   <children>
      <GridPane layoutX="142.0" layoutY="72.0" prefHeight="139.0" prefWidth="318.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="206.0" minWidth="10.0" prefWidth="75.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="354.0" minWidth="10.0" prefWidth="243.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Nom">
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </Label>
            <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Stock" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </Label>
            <TextField fx:id="nameLabel" prefHeight="30.0" GridPane.columnIndex="1" />
            <TextField fx:id="stockLabel" prefHeight="30.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <CheckBox fx:id="isActiveCheckBox" layoutX="173.0" layoutY="236.0" mnemonicParsing="false" text="Actif">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
      <Button fx:id="confirm" layoutX="301.0" layoutY="350.0" mnemonicParsing="false" onAction="#submit" prefWidth="80.0" text="Confirmer" />
      <Button fx:id="cancel" layoutX="424.0" layoutY="350.0" mnemonicParsing="false" onAction="#cancel" prefWidth="80.0" text="Annuler" />
      <Label layoutX="261.0" layoutY="29.0" text="Produit">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
